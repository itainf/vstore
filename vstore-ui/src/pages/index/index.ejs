    <style>
      .sidebar-item > .sidebar {
        top: 0;
        margin-left: 20px;
        left: 100%;
        -webkit-border-radius: 0 6px 6px 6px;
        -moz-border-radius: 0 6px 6px;
        border-radius: 0 6px 6px 6px;
        /*display: none;*/
        background-color: #1d2433;
        color: white;
      }




      .sidebar-item > a:after {
        border-left-color: #fff;
      }

      a.sidebar-link:after {
        display: block;
        content: " ";
        float: right;
        width: 0;
        height: 0;
        border-color: transparent;
        border-style: solid;
        border-width: 5px 0 5px 5px;
        border-left-color: #ccc;
        margin-top: 5px;
        margin-right: 0px;
      }


      a.sidebarActive:after {
        display: block;
        content: " ";
        float: right;
        width: 0;
        height: 0;
        border-color: transparent;
        border-style: solid;
        border-width: 5px 5px 0px 5px;
        border-top-color: #ccc;
        margin-top: 5px;
        margin-right: 0px;
      }




      .fist-nav  > .sidebar-item > a.sidebarActive  {
        color: #ffffff;
        background-color: #2580e8!important;
      }


      .fist-nav a{
        color: #8aa4af;
      }
      .fist-nav  > .sidebar-item  > .sidebar-link:hover   {
        background-color: #2580e8!important;
      }

      .sidebar-link:hover   {
        color: white;
      }

      .sidebar  a:hover{
        color: white;
      }

      .fist-nav   .sidebar-item {
        border-style: solid;
        border-width: 1px;
        border-color: transparent;
      }




      .sidebar-fixed {
        position: fixed;
        /*top: 50px;*/
        left: 0;
        z-index: 99;
        height: 100%;
        overflow: auto;
        overflow-x: hidden;
        box-shadow: 2px 0 8px 0 rgba(29, 35, 41, 0.05);


      }

      .sidebar-layout-mix {
        /*padding-top: 48px;*/
      }

      .sidebar-light {
        background: #fff;
      }



      .sidebar{
        margin: 0;
        padding: 0;
      }

      .show-nav {
        transform:translateX(0);
      }
      .hide-nav {
        transform:translateX(-220px);
      }

      .btn{
          margin-left: 10px;
      }

      .table{
        margin-bottom: 0.3rem;
      }

      .my-button-group{
        margin-left:auto;
      }



      .table td, .table th {
        border-bottom: 1px solid #dee2e6;
        vertical-align: bottom;
      }
      .table > tbody > tr:hover {
        background: #fafafa;
      }


      .content-layout{
        display: flex;
        flex: auto;
        flex-direction: column;
        min-height: 0;
        background: #f0f2f5;
      }



      .fa {
        display: inline-block;
        font-size: inherit;
        text-rendering: auto;
        -webkit-font-smoothing: antialiased;
      }

      .fa-book:before {
        content: "\f02d";
      }

      .fa.pull-right {
        margin-left: .3em;
      }


      .row {
        display: -ms-flexbox;
        display: flex;
        -ms-flex-wrap: wrap;
        flex-wrap: wrap;
        /*margin-right: -15px;*/
        /*margin-left: -15px;*/
      }
      .my-bg-dark{
        background-color: #1d2433;
      }
      .bg-dark{
        background-color: #1d2433!important;
      }
    </style>


  </head>
