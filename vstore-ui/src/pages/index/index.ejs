  <script>
    $.ajax({
      url:"/vstore/user/queryUser",
      async:false,
      success:function(result){
        const  formJson=JSON.parse(result);
        if(formJson.resCode===403){
          window.location="/login.html";
        }
      }});



  </script>
  <script>

      $(function(){
          $(".sidebar-link").click(function () {
              var navLink = $(this);
              var  nav= navLink.next();
              if (navLink.hasClass("sidebarActive")) {
                  setTimeout(function () {
                      nav.addClass("hide-nav").removeClass("show-nav");
                      navLink.removeClass("sidebarActive");
                      nav.hide();
                  }, 100)
              }
              else {
                  setTimeout(function (){
                      // $(".nav-link").removeClass("active");
                      nav.addClass("show-nav").removeClass("hide-nav");
                      navLink.addClass("sidebarActive");
                      nav.show();
                  }, 100)
              }
          })
      });


  </script>
<% for (var chunk in htmlWebpackPlugin.files.chunks) { %>
<script type="text/javascript" src="<%=htmlWebpackPlugin.files.chunks[chunk].entry %>"></script>
<% } %>
